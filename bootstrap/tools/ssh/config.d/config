Host windows home
  HostName mstrasiotto.hopto.org
  Port 2222
  ForwardX11 yes
  ForwardAgent yes

Host home
  User matty
Host windows
  User Matthew
# Install the following on windows:
# https://github.com/shayne/go-wsl2-host
Host *.wsl
  ProxyJump windows

Host cronnit
  HostName 127.0.0.1
  User cronnit
  Port 2222
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile /home/matty/projects/cronnit.us/.vagrant/machines/default/virtualbox/private_key
  IdentitiesOnly yes
  LogLevel FATAL
  ForwardX11 yes

